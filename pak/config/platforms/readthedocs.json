{
    "name": "readthedocs",
    "type": "documentation",
    "language": "markdown",
    "registry": "https://readthedocs.org",
    "api": "https://api.readthedocs.org",
    "health_endpoint": "https://readthedocs.org/health",
    "commands": {
        "publish": "readthedocs publish",
        "info": "readthedocs info {package}",
        "versions": "readthedocs versions {package}",
        "install": "readthedocs install",
        "build": "readthedocs build",
        "test": "readthedocs test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "READTHEDOCS_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["readthedocs install", "readthedocs test", "readthedocs build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
