{
    "name": "azure",
    "type": "cloud",
    "language": "terraform",
    "registry": "https://azure.org",
    "api": "https://api.azure.org",
    "health_endpoint": "https://azure.org/health",
    "commands": {
        "publish": "azure publish",
        "info": "azure info {package}",
        "versions": "azure versions {package}",
        "install": "azure install",
        "build": "azure build",
        "test": "azure test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "AZURE_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["azure install", "azure test", "azure build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
