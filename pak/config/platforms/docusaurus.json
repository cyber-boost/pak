{
    "name": "docusaurus",
    "type": "documentation",
    "language": "markdown",
    "registry": "https://docusaurus.org",
    "api": "https://api.docusaurus.org",
    "health_endpoint": "https://docusaurus.org/health",
    "commands": {
        "publish": "docusaurus publish",
        "info": "docusaurus info {package}",
        "versions": "docusaurus versions {package}",
        "install": "docusaurus install",
        "build": "docusaurus build",
        "test": "docusaurus test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "DOCUSAURUS_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["docusaurus install", "docusaurus test", "docusaurus build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
