{
    "name": "goproxy",
    "type": "go",
    "language": "go",
    "registry": "https://goproxy.org",
    "api": "https://api.goproxy.org",
    "health_endpoint": "https://goproxy.org/health",
    "commands": {
        "publish": "goproxy publish",
        "info": "goproxy info {package}",
        "versions": "goproxy versions {package}",
        "install": "goproxy install",
        "build": "goproxy build",
        "test": "goproxy test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "GOPROXY_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["goproxy install", "goproxy test", "goproxy build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
