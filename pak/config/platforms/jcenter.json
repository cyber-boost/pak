{
    "name": "jcenter",
    "type": "java",
    "language": "java",
    "registry": "https://jcenter.org",
    "api": "https://api.jcenter.org",
    "health_endpoint": "https://jcenter.org/health",
    "commands": {
        "publish": "jcenter publish",
        "info": "jcenter info {package}",
        "versions": "jcenter versions {package}",
        "install": "jcenter install",
        "build": "jcenter build",
        "test": "jcenter test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "JCENTER_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["jcenter install", "jcenter test", "jcenter build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
