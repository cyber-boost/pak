{
    "name": "jsdelivr",
    "type": "javascript",
    "language": "javascript",
    "registry": "https://jsdelivr.org",
    "api": "https://api.jsdelivr.org",
    "health_endpoint": "https://jsdelivr.org/health",
    "commands": {
        "publish": "jsdelivr publish",
        "info": "jsdelivr info {package}",
        "versions": "jsdelivr versions {package}",
        "install": "jsdelivr install",
        "build": "jsdelivr build",
        "test": "jsdelivr test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "JSDELIVR_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["jsdelivr install", "jsdelivr test", "jsdelivr build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
