{
    "name": "cocoapods",
    "type": "mobile",
    "language": "swift",
    "registry": "https://cocoapods.org",
    "api": "https://api.cocoapods.org",
    "health_endpoint": "https://cocoapods.org/health",
    "commands": {
        "publish": "cocoapods publish",
        "info": "cocoapods info {package}",
        "versions": "cocoapods versions {package}",
        "install": "cocoapods install",
        "build": "cocoapods build",
        "test": "cocoapods test"
    },
    "files": {
        "required": ["package.json"],
        "optional": ["README.md", "LICENSE"]
    },
    "authentication": {
        "type": "token",
        "env_var": "COCOAPODS_TOKEN"
    },
    "deployment": {
        "pre_hooks": ["cocoapods install", "cocoapods test", "cocoapods build"],
        "publish_flags": [],
        "rollback_support": false
    }
}
