{
  "name": "pak-sh",
  "version": "2.0.0",
  "description": "PAK.sh - Universal Package Automation Kit",
  "main": "pak-sh",
  "bin": {
    "pak-sh": "./pak-sh",
    "pak": "./pak-sh"
  },
  "scripts": {
    "install": "chmod +x pak-sh",
    "postinstall": "echo 'PAK.sh  installed. Run \"pak-sh install\" to install PAK.sh.'",
    "test": "echo 'Testing PAK.sh ...' && ./pak-sh --help"
  },
  "keywords": [
    "pak",
    "package",
    "automation",
    "deployment",
    "npm",
    "pypi",
    "cargo",
    "nuget",
    "packagist",
    "docker",
    "cli",
    "tool",
    ""
  ],
  "author": "PAK.sh Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyber-boost/pak.git"
  },
  "homepage": "https://pak.sh",
  "bugs": {
    "url": "https://github.com/cyber-boost/pak/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "preferGlobal": true,
  "files": [
    "pak-sh",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
} 